{
  "$schema": "https://json.schemastore.org/eslintrc.json",
  "extends": ["next/core-web-vitals", "prettier", "airbnb"],
  "rules": {
    "no-var": "error",
    "no-unused-vars": "error",
    "complexity": ["warn", 20],
    "curly": ["error", "multi-line"],
    "no-loop-func": "error",
    "no-bitwise": "error"
  }
}
